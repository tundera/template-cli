{
  "name": "template-cli",
  "version": "0.1.0",
  "author": "tundera<https://github.com/tundera>",
  "scripts": {
    "dev": "esno -r dotenv/config -r tsconfig-paths/register src/index.ts",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "clean": "rimraf dist output.txt",
    "release": "bumpp --tag --commit --push && npm publish",
    "prepublishOnly": "yarn build"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "tundera/template-cli"
  },
  "homepage": "https://github.com/tundera/template-cli#readme",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": "bin/cli.js",
  "devDependencies": {
    "@antfu/utils": "0.5.0",
    "@types/eslint": "^8.4.1",
    "@types/fs-extra": "9.0.13",
    "@types/node": "17.0.19",
    "@types/node-fetch": "2.6.1",
    "@types/sharp": "0.29.5",
    "@types/yargs": "17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "bumpp": "^7.1.1",
    "dotenv": "16.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "esno": "0.14.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "tsconfig-paths": "^3.12.0",
    "tsup": "5.11.13",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "cac": "^6.7.12",
    "fs-extra": "^10.0.1",
    "image-data-uri": "^2.0.1",
    "node-fetch": "3",
    "sharp": "^0.30.1"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "packageManager": "yarn@3.2.0"
}
