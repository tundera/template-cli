{
  "name": "template-cli",
  "version": "0.1.0",
  "author": "tundera<https://github.com/tundera>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tundera/template-cli"
  },
  "homepage": "https://github.com/tundera/template-cli#readme",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "program": "./bin/program.js"
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "dev": "esno src/cli.ts",
    "lint": "eslint --ext .ts,.js .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rimraf dist program",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "prepublishOnly": "pnpm build",
    "prepare": "husky install",
    "commit": "cz"
  },
  "dependencies": {
    "consola": "2.15.3",
    "dotenv": "16.0.1",
    "fs-extra": "10.0.1",
    "ohmyfetch": "0.4.15",
    "picocolors": "1.0.0",
    "unconfig": "0.3.11",
    "yargs": "17.3.1"
  },
  "devDependencies": {
    "@types/eslint": "8.4.1",
    "@types/fs-extra": "9.0.13",
    "@types/node": "17.0.21",
    "@types/yargs": "17.0.10",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "bumpp": "7.1.1",
    "commitizen": "4.2.4",
    "cz-emoji": "1.3.1",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "esno": "0.14.1",
    "husky": "7.0.4",
    "jiti": "1.13.0",
    "lint-staged": "12.3.7",
    "pinst": "3.0.0",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "rimraf": "3.0.2",
    "tsconfig-paths": "3.14.1",
    "tsup": "5.12.1",
    "typescript": "4.7.3",
    "unbuild": "0.7.0",
    "vitest": "0.7.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "typescript": "*"
      }
    }
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "lint-staged": {
    "*.{js,ts}": [
      "pnpm lint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "packageManager": "pnpm@6.32.3"
}
